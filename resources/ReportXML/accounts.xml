<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<sheet id="sheet1" sheetName="sheet1">
		<style>
			<colWidth>
				<widthItem>3000</widthItem>
				<widthItem>3000</widthItem>
				<widthItem>3000</widthItem>
				<widthItem>3000</widthItem>
				<widthItem>5000</widthItem>
			</colWidth>
		</style>
		<!-- 定义动态表头 -->
		<dynamicReportHeader>
			<!-- 动态可变长表头 -->
			<dynamicLengthReportHeader>
				<header>
					<require>
						dynamicLengt1
					</require>
				</header>
			</dynamicLengthReportHeader>

			<!-- 静态单表头 -->
			<staticSingleReportHeader>

				<header id="header1">
					<address>
						<row>1</row>
						<col>0</col>
					</address>
					<title>合同编码</title>
				</header>

				<header id="header2">
					<address>
						<row>1</row>
						<col>1</col>
					</address>
					<title>合同名称</title>
				</header>

				<header id="header3">
					<address>
						<row>1</row>
						<col>2</col>
					</address>
					<title>合同金额</title>
				</header>
				<header id="header3">
					<address>
						<row>1</row>
						<col>3</col>
					</address>
					<title>报账专员</title>
				</header>
				<header id="header4">
					<address>
						<row>1</row>
						<col>4</col>
					</address>
					<title>录入时间</title>
				</header>
			</staticSingleReportHeader>
		</dynamicReportHeader>
		<!-- 表格数据体 -->
		<dataArea>
			<!-- 起始点 -->
			<basePoint>
				<row>2</row>
				<col>0</col>
			</basePoint>

			<!-- 数据项 -->
			<dataItem>
				<item>contractCode</item>
				<item>contractName</item>
				<item>contractAmount</item>
				<item>employeeName</item>
				<item>checkinTime</item>
			</dataItem>
		</dataArea>
	</sheet>
</workbook>

